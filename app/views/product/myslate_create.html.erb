<script src="http://static.filmquotra.com/assets/res/js/filmquotra.min.js"></script>
<script src="http://external.filmquotra.com/assets/res/js/validation.js"></script>
<script src="http://external.filmquotra.com/assets/res/js/createProject.js"></script>

<script src="http://external.filmquotra.com/assets/res/js/send_request.js"></script>
</head>
<body>
	

		<div id="header">
			<h1><a href="/">filmquotra</a></h1>		
		</div>
		<div id="user-nav" class="navbar navbar-inverse">
            <ul class="nav btn-group">
                
				<li class="btn btn-inverse"><a title="" href="/profile/changepassword"><i class="icon icon-user"></i> <span class="text">Profile</span></a></li>
                <li class="btn btn-inverse"><a title="" href="/logout"><i class="icon icon-share-alt"></i> <span class="text">Logout</span></a></li>
            </ul>
        </div>
            
		<div id="sidebar">
		
			
			
				<a href="/myslate/create" class="visible-phone"><i
					class="icon icon-th-list"></i>Create Project</a>
			
			
			
			
		
		<ul>
				<li ><a href="/myslate/"><i class="icon icon-home"></i> <span>MySlate</span></a></li>
				<li class="active"><a href="/myslate/create"><i class="icon icon-home"></i> <span>Create Project</span></a></li>
				<li ><a href=""><i class="icon icon-home"></i> <span>Comps</span></a></li>
				<li ><a href=""><i class="icon icon-home"></i> <span>Ultimates</span></a></li>
				<li ><a href="/help"><i class="icon-question-sign"></i> <span>Help</span></a></li>
			</ul>
		</div>






    
    <div id="content">

    	
	<div id="content-header">
		<h1>Let's get started!</h1>
	</div>
	
	
	<div id="breadcrumb">
		<a href="/" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a>
		
		<a href="/myslate">Project</a>
		
		<a href="/myslate/create" class="current">Create</a>
		
	</div>
	
	
	<div class="container-fluid">
		
		
	    
		<div class="row-fluid">
			<div class="span9">
			


<div class="widget-box">
	<div class="widget-title">
		<span class="icon"> <i class="icon-align-justify"></i>
		</span>
		<h5>Name your project.  All great films start off with a working title.<a href="/myslate"><id="fqAsterisk">*</id>More...</a></h5>
	</div>
	<div class="widget-content nopadding">
		<form class="form-horizontal" method="post" action="/myslate/create"
			id="createForm" novalidate="novalidate">
			<div class="control-group">
				<a class="accordion-toggle" data-toggle="collapse" data-parent="#accordion2" href="#collapseOne">
					<label class="control-label">Project Title* </label>
				</a>
				<div class="controls">
					<input type="text" name="title" id="title" value="" />
				</div>
			</div>
			<div id="collapseOne" class="accordion-body collapse">
				<div class="accordion-inner">
				  <div class="fqAsterisk">*</div>
				  <p class="fqTypeface">
					 Don't worry if you don't have the perfect title yet.  These things can and often will change throughout development and production.  For now, your project title will not affect any of the financial metrics, and is for tracking purposes only.
				 </p>
				</div>
			</div>

<!--create button-->
			<div class="form-actions">
				<div class="btn btn-success" style="width: 160px; height: 40px">
					<%= link_to "Create", myslate_create2_path %>
				</div>
			</div>
		</form>
	</div>
</div>
			</div>
			<div class="span3">
			


<div class="widget-box">
	<div class="widget-title">
		<span class="icon">
			<i class=" icon-question-sign"></i>
		</span>
		<h5>Ask us anything!</h5>
	</div>
	<div class="widget-content nopadding">
		<form class="form-horizontal" method="post" action=""
			id="sendRequestForm" novalidate="novalidate">
			<div class="control-group">
				<!-- <label class="control-label" for="sendRequestTitle">Title</label> -->
				<div class="controls" style="margin-left:35px">
					<input type="text" name="sendRequestTitle" id="sendRequestTitle" value="" />
				</div>
			</div>
			<div class="control-group">
				<!-- <label class="control-label" for="sendRequestContents">Contents</label> -->
				<div class="controls" style="margin-left:35px">
					<textarea rows="" cols="" id="sendRequestContents" name="sendRequestContents"></textarea>
				</div>
			</div>

			<div class="form-actions">
				
				<span id="sendRequestWrap" class="pull-right"><button class="btn btn-info" id="sendRequestBtn">Send</button></span>
                <span class="pull-right" style="display:none" id="sendRequestLoadingWrap"><img src="http://static.filmquotra.com/assets/res/img/loader.gif" /></span>
			</div>
			<input type="hidden" name="sendRequestUrl" id="sendRequestUrl" value='/myslate/create' />
			
		</form>
	</div>
</div>


			</div>
		</div>	      
	    
	    
		
		<div class="row-fluid">
			<div id="footer" class="span12">
			</div>
		</div>
	</div>
	
	

    </div>
    <div class="footer">
		<div class="footer-inner">
			<div class="container">
				<div class="row">
					<div class="span12">
						&copy; 2012 <a href="http://filmquotra.com">filmquotra</a>.
					</div>
				</div>
			</div>
		</div>
	</div>

